// Напишите программу, которая задаёт массив из 8 элементов и выводит их на экран.

// 1, 2, 5, 7, 19 -> [1, 2, 5, 7, 19]

// 6, 1, 33 -> [6, 1, 33]

int[] array =  new int[8]; //объявили масссив из 8 элементов

int length = 9;
int index = 0;

while(index<length)
{
index = new Random() .Next();  // заполнить массив числами, от 1 до 10 рандомно
index++;
System.Console.WriteLine(index );
}

